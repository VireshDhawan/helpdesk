<!DOCTYPE html>
<html>
	<head>
	  <title>Helpdesk</title>
		<meta charset="utf-8">
		<meta content="width=device-width, initial-scale=1.0" name="viewport">
	  <%= stylesheet_link_tag  "super_admin", media: "all" %>
	  <%= javascript_include_tag "super_admin" %>
	  <%= csrf_meta_tags %>
	  <%= render 'layouts/shim' %>
	  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,800,800italic|Oswald:400,300,700' rel='stylesheet' type='text/css'>
	  <link href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
	</head>
	<body class=" ">
		<%= render 'layouts/super_admin_header' %>
		<%= render 'layouts/super_admin_navigation' %>
		<%= render 'layouts/errors' %>

		<%= yield %>

		<%= render 'layouts/small_footer' %>
	</body>
</html>
