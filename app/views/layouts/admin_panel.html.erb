<!DOCTYPE html>
<html>
<head>
  <title>Helpdesk</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,800,800italic|Oswald:400,300,700' rel='stylesheet' type='text/css'>
</head>
<body>
	<%= render 'layouts/control_panel_header' %>
	<%= render 'layouts/errors' %>
	<div class="row" id="admins_cp_sidebar">
		<div class="col-sm-2">
			<%= render 'layouts/admin_sidebar' %>
		</div>
		<div class="col-sm-10" id="admins_cp_body">
			<%= yield %>
		</div>
	</div>
	<%= render 'layouts/control_panel_footer' %>
</body>
</html>
