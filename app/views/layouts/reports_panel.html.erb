<!DOCTYPE html>
<html>
<head>
  <title>Helpdesk</title>
  <%= include_gon(:init => true) %>
  <%= stylesheet_link_tag    "reports", media: "all" %>
  <%= javascript_include_tag "reports" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,800,800italic|Oswald:400,300,700' rel='stylesheet' type='text/css'>
</head>
<body>
	<%= render 'layouts/control_panel_header_main' %>
	<%= render 'layouts/reports_navigation' %>
	<%= render 'layouts/errors' %>
	<%= yield %>
	<%= render 'layouts/small_footer' %>
</body>
</html>
